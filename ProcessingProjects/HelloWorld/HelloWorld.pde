void draw() {


  size(500, 500);

  quad(250, 0, 0, 250, 500, 250, 250, 500);
  quad(250, 500, 0, 250, 500, 250, 250, 0);

  line(0, 0, 250, 250);
  line(0, 500, 250, 250);
  line(500, 500, 250, 250);
  line(500, 0, 250, 250);

  circle(250, 250, 100);

  line(125, 125, 375, 125);
  line(125, 125, 125, 375);
  line(125, 375, 375, 375);
  line(375, 375, 375, 125);
  circle(250, 250, x);

  if (x > 0) {
    x = x-1;
  } else {
    x = 500;
  }

  circle(250, 250, y);

  if (y < 500) {
    y = y+1;
  } else {
    y = 0;
  }
}
